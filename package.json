{
  "name": "breakpirates-v3",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run supportedBrowsers && ng build",
    "build:prod": "npm run supportedBrowsers && ng build --prod --stats-json",
    "build:stats": "webpack-bundle-analyzer dist/breakpirates-v3/stats.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cc": "jest --coverage",
    "lint": "ng lint",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > supportedBrowsers.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.7",
    "@angular/common": "^11.2.7",
    "@angular/compiler": "^11.2.7",
    "@angular/core": "^11.2.7",
    "@angular/forms": "^11.2.7",
    "@angular/platform-browser": "^11.2.7",
    "@angular/platform-browser-dynamic": "^11.2.7",
    "@angular/router": "^11.2.7",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.6.0",
    "luxon": "^1.26.0",
    "mediaelement": "^4.2.16",
    "ngx-bootstrap": "^6.2.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.6",
    "@angular-eslint/builder": "^2.0.0",
    "@angular-eslint/eslint-plugin": "^2.0.0",
    "@angular-eslint/eslint-plugin-template": "^2.0.0",
    "@angular-eslint/schematics": "^2.0.2",
    "@angular-eslint/template-parser": "^2.0.0",
    "@angular/cli": "^11.2.6",
    "@angular/compiler-cli": "^11.2.7",
    "@angular/language-service": "^11.2.7",
    "@types/jest": "^26.0.22",
    "@types/luxon": "^1.26.2",
    "@types/node": "^12.20.7",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "browserslist-useragent-regexp": "^3.0.0",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "shallow-render": "^11.0.2",
    "ts-node": "~8.3.0",
    "typescript": "^4.1.5",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ]
  }
}
