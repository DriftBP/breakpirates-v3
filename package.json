{
  "name": "breakpirates-v3",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run supportedBrowsers && ng build",
    "build:prod": "npm run supportedBrowsers && ng build",
    "build:prod:stats": "npm run supportedBrowsers && ng build --stats-json",
    "build:stats": "webpack-bundle-analyzer dist/breakpirates-v3/stats.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cc": "jest --coverage",
    "lint": "ng lint",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > supportedBrowsers.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/forms": "^13.3.11",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/router": "^13.3.11",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.2.0",
    "luxon": "^3.0.1",
    "mediaelement": "^5.0.5",
    "ngx-bootstrap": "^8.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.9",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "^13.3.9",
    "@angular/compiler-cli": "^13.3.11",
    "@angular/language-service": "^13.3.11",
    "@types/jest": "^27.5.2",
    "@types/luxon": "^2.4.0",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "browserslist-useragent-regexp": "^3.0.2",
    "eslint": "^8.20.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.2",
    "shallow-render": "^13.0.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.4.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ]
  }
}
