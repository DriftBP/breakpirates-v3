{
  "name": "breakpirates-v3",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run supportedBrowsers && ng build",
    "build:prod": "npm run supportedBrowsers && ng build --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cc": "jest --coverage",
    "lint": "ng lint",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > supportedBrowsers.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.6.0",
    "luxon": "^1.26.0",
    "mediaelement": "^4.2.16",
    "ngx-bootstrap": "^6.2.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.0",
    "@angular-eslint/builder": "0.6.0-beta.0",
    "@angular-eslint/eslint-plugin": "0.6.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "0.6.0-beta.0",
    "@angular-eslint/schematics": "^0.6.0-beta.0",
    "@angular-eslint/template-parser": "0.6.0-beta.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.25.2",
    "@types/node": "^12.20.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "browserslist-useragent-regexp": "^2.1.1",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "shallow-render": "^11.0.2",
    "ts-node": "~8.3.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ]
  }
}
